install.packages("moments")
library(moments)
getmode <- function(x) {
  uniq <- unique(x)
  uniq[which.max(tabulate(match(x, uniq)))]
}

#Q1 Calories Consumed
calories_consumed <- read.csv("C:/Users/Lenovo/Downloads/calories_consumed.csv")
View(calories_consumed)
attach(calories_consumed)
Wt_Gain = calories_consumed$Weight.gained..grams.
Cal_Cons = calories_consumed$Calories.Consumed
mean(Wt_Gain)
median(Wt_Gain)
getmode(Wt_Gain)
sd(Wt_Gain)
var(Wt_Gain)
quantile(Wt_Gain)
skewness(Wt_Gain)
kurtosis(Wt_Gain)
hist(Wt_Gain)
boxplot(Wt_Gain)
boxplot(Wt_Gain, horizontal = TRUE)
plot(Cal_Cons, Wt_Gain)
cor(Cal_Cons, Wt_Gain)
Weight_model <- lm(Wt_Gain ~ Cal_Cons)
summary(Weight_model)
Weight_model_log <- lm(Wt_Gain ~ log(Cal_Cons))
summary(Weight_model_log)
Weight_model_exp <- lm(log(Wt_Gain) ~ Cal_Cons)
summary(Weight_model_exp)
plot(Weight_model)

#Q2 Delivery Time
delivery_time <- read.csv("C:/Users/Lenovo/Downloads/delivery_time.csv")
View(delivery_time)
attach(delivery_time)
DT = delivery_time$Delivery.Time
ST = delivery_time$Sorting.Time 
mean(DT)
median(DT)
getmode(DT)
sd(DT)
var(DT)
quantile(DT)
skewness(DT)
kurtosis(DT)
hist(DT)
boxplot(DT)
boxplot(DT, horizontal = TRUE)
plot(ST, DT)
cor(ST, DT)
Delivery_model <- lm(DT ~ ST)
summary(Delivery_model)
Delivery_model_log <- lm(DT ~ log(ST))
summary(Delivery_model_log)
Delivery_model_exp <- lm(log(DT) ~ ST)
summary(Delivery_model_exp)
plot(Delivery_model_exp)

#Q3 Emp data
emp_data <- read.csv("C:/Users/Lenovo/Downloads/emp_data.csv")
View(emp_data)
attach(emp_data)
SH = Salary_hike
CR = Churn_out_rate
mean(CR)
median(CR)
getmode(CR)
sd(CR)
var(CR)
quantile(CR)
skewness(CR)
kurtosis(CR)
hist(CR)
boxplot(CR)
boxplot(CR, horizontal = TRUE)
plot(SH, CR)
cor(SH, CR)
Churnout_model <- lm(CR ~ SH)
summary(Churnout_model)
Churnout_model_log <- lm(CR ~ log(SH))
summary(Churnout_model_log)
Churnout_model_exp <- lm(log(CR) ~ SH)
summary(Churnout_model_exp)
plot(Churnout_model_exp)

#Q4 Salary Hike
Salary_Data <- read.csv("C:/Users/Lenovo/Downloads/Salary_Data.csv")
View(Salary_Data)
attach(Salary_Data)
SAL = Salary
YE = YearsExperience
mean(SAL)
median(SAL)
getmode(SAL)
sd(SAL)
var(SAL)
quantile(SAL)
skewness(SAL)
kurtosis(SAL)
hist(SAL)
boxplot(SAL)
boxplot(SAL, horizontal = TRUE)
plot(YE, SAL)
cor(YE, SAL)
Salary_model <- lm(SAL ~ YE)
summary(Salary_model)
Salary_model_log <- lm(SAL ~ log(YE))
summary(Salary_model_log)
Salary_model_exp <- lm(log(SAL) ~ YE)
summary(Salary_model_exp)
plot(Salary_model)
