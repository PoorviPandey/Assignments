#Q1 Calories Consumed
calories_consumed <- read.csv("C:/Users/Lenovo/Downloads/calories_consumed.csv")
View(calories_consumed)
attach(calories_consumed)
mean(Weight.gained..grams.)
sd(Weight.gained..grams.)
median(Weight.gained..grams.)
var(Weight.gained..grams.)
install.packages("moments")
library(moments)
skewness(Weight.gained..grams.)
kurtosis(Weight.gained..grams.)
hist(Weight.gained..grams.)
boxplot(Weight.gained..grams.)
boxplot(Weight.gained..grams., horizontal = TRUE)
summary(Weight.gained..grams.)
range(Weight.gained..grams.)
plot(Calories.Consumed, Weight.gained..grams.)
cor(Calories.Consumed, Weight.gained..grams.)
Weight_model <- lm(Weight.gained..grams. ~ Calories.Consumed)
summary(Weight_model)

#Q2 Delivery Time
delivery_time <- read.csv("C:/Users/Lenovo/Downloads/delivery_time.csv")
View(delivery_time)
attach(delivery_time)
mean(Delivery.Time)
median(Delivery.Time)
var(Delivery.Time)
sd(Delivery.Time)
library(moments)
skewness(Delivery.Time)
kurtosis(Delivery.Time)
hist(Delivery.Time)
boxplot(Delivery.Time)
boxplot(Delivery.Time, horizontal = TRUE)
summary(Delivery.Time)
range(Delivery.Time)
plot(Sorting.Time, Delivery.Time)
cor(Sorting.Time, Delivery.Time)
Delivery_model <- lm(Delivery.Time ~ Sorting.Time)
summary(Delivery_model)
Delivery_model_log <- lm(Delivery.Time ~ log(Sorting.Time))
summary(Delivery_model_log)
Delivery_model_exp <- lm(log(Delivery.Time) ~ Sorting.Time)
summary(Delivery_model_exp)
plot(Sorting.Time, log(Delivery.Time))
cor(Sorting.Time, log(Delivery.Time))

#Q3 Emp data
emp_data <- read.csv("C:/Users/Lenovo/Downloads/emp_data.csv")
View(emp_data)
attach(emp_data)
mean(Salary_hike)
median(Salary_hike)
var(Salary_hike)
sd(Salary_hike)
library(moments)
skewness(Salary_hike)
kurtosis(Salary_hike)
hist(Salary_hike)
boxplot(Salary_hike)
boxplot(Salary_hike, horizontal = TRUE)
summary(Salary_hike)
range(Salary_hike)
plot(Churn_out_rate, Salary_hike)
cor(Churn_out_rate, Salary_hike)
Salary_hike_model <- lm(Salary_hike ~ Churn_out_rate)
summary(Salary_hike_model)
Salary_hike_model_log <- lm(Salary_hike ~ log(Churn_out_rate))
summary(Salary_hike_model_log)
Salary_hike_model_exp <- lm(log(Salary_hike) ~ Churn_out_rate)
summary(Salary_hike_model_exp)

#Q4 Salary Hike
Salary_Data <- read.csv("C:/Users/Lenovo/Downloads/Salary_Data.csv")
View(Salary_Data)
attach(Salary_Data)
mean(Salary)
median(Salary)
var(Salary)
sd(Salary)
library(moments)
skewness(Salary)
kurtosis(Salary)
hist(Salary)
boxplot(Salary)
boxplot(Salary, horizontal = TRUE)
summary(Salary)
range(Salary)
plot(YearsExperience, Salary)
cor(YearsExperience, Salary)
Salary_model <- lm(Salary ~ YearsExperience)
summary(Salary_model)
Salary_model_log <- lm(Salary ~ log(YearsExperience))
summary(Salary_model_log)
Salary_model_exp <- lm(log(Salary) ~ YearsExperience)
summary(Salary_model_exp)
